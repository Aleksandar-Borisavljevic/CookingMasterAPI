{
  "UseInMemoryDatabase": false,
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "d:\\home\\LogFiles\\rawLogs\\log.txt",
          "rollingInterval": "Day",
          "shared": "true",
          "flushToDiskInterval": "0:00:05",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {SourceContext}{NewLine}{Message:lj}{NewLine}{Exception}{NewLine}",
          "retainedFileCountLimit": "72",
          "restrictedToMinimumLevel": "Warning"
        }
      }
    ]
  }
}
